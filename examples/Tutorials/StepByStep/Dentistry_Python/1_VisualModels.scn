<!-- 
Step 1:
Visual Anatomy
-->
<Node name="root" dt="0.05">

    <BackgroundSetting color="0.5 0.5 0.5" />
    <VisualStyle displayFlags="showVisualModels hideForceFields" /> 

    <Node name="Head">

	<Node name="Head Visual Model" >
	    <VisualStyle displayFlags="showVisualModels " /> 
 	    <MeshObjLoader name="meshLoader0" filename="data/mesh/head.obj" />
 	    <OglModel name="HeadVisualModel" src="@meshLoader0" texturename="Tutorials/StepByStep/Dentistry_Haptics/data/pics/head_baseTexBaked.png" />
	</Node>

        <Node name="Tongue" activated="true">
            <Node name="TongueVisualModel">
                <MeshObjLoader name="meshLoader1" filename="data/mesh/tongue.obj" />
                <OglModel name="TongueVisualModel" src="@meshLoader1" texturename="Tutorials/StepByStep/Dentistry_Haptics/data/pics/tongue_color.png" />
            </Node>
        </Node>

        <Node name="Teeth" >
            <Node name="VisualModel">
                <MeshObjLoader name="meshLoader2" filename="data/mesh/teeth.obj" />
                <OglModel name="TeethVisualModel" src="@meshLoader2" texturename="Tutorials/StepByStep/Dentistry_Haptics/data/pics/teeth_color.png" />
            </Node>
        </Node>
    </Node>
s</Node>
